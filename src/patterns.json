[
  {
    "name": "Basic Year/Number/EC or EU reference",
    "pattern": "\\b(\\d{2,4})\\/(\\d{1,4})\\/(EC|EU)\\b",
    "type": "L"
  },
  {
    "name": "(EU|EC) prefix format (e.g. (EU) 2015/652)",
    "pattern": "\\(?(EU|EC)\\)?\\s*(\\d{4})\\/(\\d{1,4})\\b",
    "type": "L"
  },
  {
    "name": "Regulation (EU) YYYY/NNNN",
    "pattern": "\\bRegulation\\s*\\(EU\\)\\s*(\\d{4})\\/(\\d{1,4})\\b",
    "type": "R"
  },
  {
    "name": "Regulations (EC) No NNN/NNNN",
    "pattern": "\\bRegulations?\\s*\\(EC\\)\\s*No\\s*(\\d{1,4})\\/(\\d{1,4})\\b",
    "type": "R"
  },
  {
    "name": "Repealing Regulation (EU) No NNN/NNNN",
    "pattern": "repealing\\s+Regulation\\s*\\(EU\\)\\s*No\\s*(\\d{1,4})\\/(\\d{1,4})\\b",
    "type": "R"
  }
]
